	<subDataset name="inventory sold" uuid="4ddff745-c801-490f-9e16-520912866ea0">
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="postgre UAT"/>
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<parameter name="AD_CLIENT_ID" class="java.lang.Integer"/>
		<parameter name="Begin Date" class="java.sql.Timestamp"/>
		<parameter name="End Date" class="java.sql.Timestamp"/>
		<queryString>
			<![CDATA[select
    billdetails.bill_date as billdate,
    billitems.item_name as item_name,
    sum(billitems.quantity) as quantity,
    billitems.price as price,
    billitems.dispensepoint as dispensepoint
from
(
select co.c_order_id,
       co.bh_visitdate as bill_date
from c_order co
where ((co.ad_client_id = $P{AD_CLIENT_ID}) AND (co.docstatus <> 'VO')
      AND co.bh_visitdate BETWEEN $P{Begin Date} AND $P{End Date}
    )
) as billdetails
inner join
(
select bh_c_order_id
from c_payment p
where ((p.ad_client_id = $P{AD_CLIENT_ID}) and ((p.docstatus = 'CO') OR (p.docstatus = 'DR' and p.bh_processing = 'Y'))
    )
group by bh_c_order_id
) as payments on billdetails.c_order_id = payments.bh_c_order_id
inner join
(
 select
  corderline.c_order_id,
  item.name as item_name,
  sum(corderline.qtyordered) as quantity,
  corderline.priceactual as price,
  dispensepoint.name as dispensepoint
from c_orderline as corderline
  inner join m_product item on corderline.m_product_id = item.m_product_id
  inner join m_warehouse dispensepoint on corderline.m_warehouse_id = dispensepoint.m_warehouse_id
  join c_order o on o.c_order_id = corderline.c_order_id
where corderline.ad_client_id = $P{AD_CLIENT_ID} and item.isactive = 'Y' and producttype = 'I'
group by corderline.c_order_id, item.name, o.bh_visitdate, corderline.priceactual, dispensepoint.name
) as billitems on payments.bh_c_order_id = billitems.c_order_id
group by billdetails.bill_date, billitems.item_name, billitems.price, billitems.dispensepoint
order by billdetails.bill_date, billitems.item_name asc;]]>
		</queryString>
		<field name="billdate" class="java.sql.Date">
			<property name="com.jaspersoft.studio.field.label" value="billdate"/>
		</field>
		<field name="item_name" class="java.lang.String">
			<property name="com.jaspersoft.studio.field.label" value="item_name"/>
			<property name="com.jaspersoft.studio.field.tree.path" value="m_product"/>
		</field>
		<field name="quantity" class="java.math.BigDecimal">
			<property name="com.jaspersoft.studio.field.label" value="quantity"/>
			<property name="com.jaspersoft.studio.field.tree.path" value="c_orderline"/>
		</field>
		<field name="price" class="java.math.BigDecimal">
			<property name="com.jaspersoft.studio.field.label" value="price"/>
			<property name="com.jaspersoft.studio.field.tree.path" value="c_orderline"/>
		</field>
		<field name="dispensepoint" class="java.lang.String">
			<property name="com.jaspersoft.studio.field.label" value="dispensepoint"/>
			<property name="com.jaspersoft.studio.field.tree.path" value="m_warehouse"/>
		</field>
		<variable name="runningTotal" class="java.math.BigDecimal" calculation="Sum">
			<variableExpression><![CDATA[$F{price}.multiply($F{quantity})]]></variableExpression>
			<initialValueExpression><![CDATA[$F{price}.multiply($F{quantity})]]></initialValueExpression>
		</variable>
	</subDataset>
