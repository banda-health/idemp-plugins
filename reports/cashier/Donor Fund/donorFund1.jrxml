	<subDataset name="dataset" uuid="4ddff745-c801-490f-9e16-520912866ea0">
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="KEUAT"/>
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<parameter name="AD_CLIENT_ID" class="java.lang.Integer"/>
		<parameter name="Begin Date" class="java.sql.Timestamp"/>
		<parameter name="End Date" class="java.sql.Timestamp"/>
		<queryString>
			<![CDATA[SELECT
	b.c_order_id,
	sorder.bh_visitdate      AS date,
	patient.name             AS name,
	patient.bh_birthday      AS DOB,
	''                       AS ke_NO,
	patient.bh_patientid     AS opd_NO,
	bh_cielname              AS diagnoses,
	(SUM(b.linenetamt)) * -1 AS amount_tendered
FROM
	bh_get_visit_non_patient_payments($P{AD_CLIENT_ID}, $P{Begin Date}, $P{End Date}) b
		INNER JOIN c_order sorder
			ON b.c_order_id = sorder.c_order_id
		INNER JOIN c_invoice invoice
			ON sorder.c_order_id = invoice.c_order_id
		INNER JOIN c_bpartner patient
			ON sorder.c_bpartner_id = patient.c_bpartner_id
		LEFT JOIN bh_coded_diagnosis diagnosis
			ON sorder.bh_primarycodeddiagnosis_id = diagnosis.bh_coded_diagnosis_id
WHERE
	invoice.docstatus != 'RE'
	AND b.bh_subtype = 'D'
GROUP BY
	sorder.bh_visitdate, patient.name, patient.bh_patientid, b.c_order_id, invoice.description, diagnosis.bh_cielname,
	patient.bh_birthday
ORDER BY
	sorder.bh_visitdate ASC;]]>
		</queryString>
