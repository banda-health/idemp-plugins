		<queryString language="SQL">
			<![CDATA[select
  sorder.bh_visitdate as date,
  patient.name as name,
  '' as ke_NO,
  patientID.bh_patientid as opd_NO,
  invoice.description as diagnoses,
  sum(payment.payamt) as amount_tendered
from c_order sorder
  inner join c_invoice invoice on sorder.c_order_id = invoice.c_order_id
  inner join ad_user patient on invoice.ad_user_id = patient.ad_user_id
  inner join ad_user cashier on sorder.salesrep_id = cashier.ad_user_id
  inner join c_bpartner patientID on patient.c_bpartner_id = patientID.c_bpartner_id
  inner join c_payment payment on invoice.c_order_id = payment.bh_c_order_id
where
	invoice.docstatus != 'RE' AND
  payment.tendertype = 'F' and payment.ad_org_id =  $P{AD_CLIENT_ID} and payment.c_invoice_id is not null
  and (date(sorder.bh_visitdate) BETWEEN  $P{Begin Date} AND  $P{End Date})
group by sorder.bh_visitdate, patient.name, patientID.bh_patientid, invoice.description
order by sorder.bh_visitdate asc;]]>
		</queryString>
