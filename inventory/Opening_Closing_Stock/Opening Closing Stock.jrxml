from m_transaction
where ad_client_id =  $P{AD_CLIENT_ID}
    and (date(movementdate) BETWEEN date($P{Begin Date}) AND date($P{End Date}))
group by m_product_id
        ) as changesinqty on currentqty.m_product_id = changesinqty.m_product_id
group by item_name
