	<subDataset name="End stock value" uuid="4ddff745-c801-490f-9e16-520912866ea0">
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="postgre UAT"/>
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<parameter name="AD_CLIENT_ID" class="java.lang.Integer"/>
		<parameter name="Begin Date" class="java.util.Date"/>
		<parameter name="End Date" class="java.util.Date"/>
		<queryString>
			<![CDATA[select productname.name as item_name,
       coalesce(beginningqty.beginning,0) as beginning,
       coalesce(endingqty.ending,0) as ending,
       abs(coalesce(sum(endingqty.ending*productSP.SalesPrice),0)) as endingvalue
from
(
select m_product_id,
       name
from m_product
where isactive = 'Y' and ad_client_id = $P{AD_CLIENT_ID}
) as productname
inner join
(
select m_product_id,
       sum(qtyonhand) as ending
from m_storageonhand
where ad_client_id = $P{AD_CLIENT_ID} and date(updated) <=  $P{End Date}
group by m_product_id
) as endingqty on productname.m_product_id = endingqty.m_product_id
left join
(
select m_product_id,
       sum(qtyonhand) as beginning
from m_storageonhand
where ad_client_id = $P{AD_CLIENT_ID} and date(updated) <=  $P{Begin Date}
group by m_product_id
) as beginningqty on productname.m_product_id = beginningqty.m_product_id
  and endingqty.m_product_id = beginningqty.m_product_id
left join
(
select
    productPrice.m_product_id,
    productPrice.pricestd as SalesPrice
    from m_pricelist priceList
    inner join m_pricelist_version priceListVersion on priceList.m_pricelist_id = priceListVersion.m_pricelist_id
    inner join m_productprice productPrice on priceListVersion.m_pricelist_version_id = productPrice.m_pricelist_version_id
where priceList.issopricelist = 'Y' and priceList.isdefault = 'Y'
) as productSP on productname.m_product_id = productSP.m_product_id
group by productname.name, coalesce(beginningqty.beginning,0),
coalesce(endingqty.ending,0)
order by productname.name asc;]]>
		</queryString>
		<field name="item_name" class="java.lang.String">
			<property name="com.jaspersoft.studio.field.label" value="item_name"/>
			<property name="com.jaspersoft.studio.field.tree.path" value="m_product"/>
		</field>
		<field name="beginning" class="java.math.BigDecimal">
			<property name="com.jaspersoft.studio.field.label" value="beginning"/>
		</field>
		<field name="ending" class="java.math.BigDecimal">
			<property name="com.jaspersoft.studio.field.label" value="ending"/>
		</field>
		<field name="endingvalue" class="java.math.BigDecimal">
			<property name="com.jaspersoft.studio.field.label" value="endingvalue"/>
		</field>
		<variable name="runningTotal" class="java.math.BigDecimal" calculation="Sum">
			<variableExpression><![CDATA[$F{endingvalue}]]></variableExpression>
			<initialValueExpression><![CDATA[$F{endingvalue}]]></initialValueExpression>
		</variable>
	</subDataset>
