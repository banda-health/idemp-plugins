	<subDataset name="End stock value" uuid="4ddff745-c801-490f-9e16-520912866ea0">
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="postgres UAT2 "/>
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<parameter name="AD_CLIENT_ID" class="java.lang.Integer"/>
		<parameter name="Begin Date" class="java.util.Date"/>
		<parameter name="End Date" class="java.util.Date"/>
		<queryString>
			<![CDATA[WITH params AS (
	SELECT
		1000000            AS ad_client_id,
		date('2022-05-06') AS begin_date,
		date('2022-05-14') AS end_date
)
SELECT
	item_name,
	endingstock,
	receivedstock,
	soldstock,
	balancestock,
	changedstock
FROM
	(
		SELECT
			productname.name                              AS item_name,
			COALESCE(SUM(currentqty.closingqty), 0)       AS endingstock,
			COALESCE(SUM(stockreceived.qtyreceived), 0)   AS receivedstock,
			COALESCE(stocksold.qtysold, 0)                AS soldstock,
			COALESCE(SUM(stocktakechange.qtybalanced), 0) AS balancestock,
			COALESCE(SUM(changesinqty.qtychanged), 0)     AS changedstock
		FROM
			(
				SELECT
					soh.m_product_id,
					soh.m_attributesetinstance_id,
					p.name
				FROM
					m_storageonhand soh
						JOIN m_product p
							ON soh.m_product_id = p.m_product_id
						JOIN params
							ON params.ad_client_id = soh.ad_client_id
				GROUP BY soh.m_product_id, p.name, soh.m_attributesetinstance_id
			) AS productname
				LEFT JOIN (
				SELECT
					m_product_id,
					m_attributesetinstance_id,
					SUM(qtyonhand) AS closingqty
				FROM
					m_storageonhand
						JOIN params
							ON params.ad_client_id = m_storageonhand.ad_client_id
						AND date(updated) <= end_date
				WHERE
					isactive = 'Y'
				GROUP BY m_product_id, m_attributesetinstance_id
			) AS currentqty
					ON currentqty.m_product_id = productname.m_product_id AND
					   currentqty.m_attributesetinstance_id = productname.m_attributesetinstance_id
				LEFT JOIN (
				SELECT
					t.m_product_id,
					t.m_attributesetinstance_id,
					SUM(t.movementqty) AS qtychanged
				FROM
					m_transaction t
						JOIN params
							ON params.ad_client_id = t.ad_client_id
						AND date(movementdate) BETWEEN begin_date AND end_date
				WHERE
					movementtype NOT IN ('C-', 'I+')
				GROUP BY t.m_product_id, t.m_attributesetinstance_id
			) AS changesinqty
					ON currentqty.m_product_id = changesinqty.m_product_id
				AND currentqty.m_attributesetinstance_id = changesinqty.m_attributesetinstance_id
				LEFT JOIN (
				SELECT
					ol.m_product_id,
					AVG(ol.priceentered) AS priceentered,
					ol.m_attributesetinstance_id,
					SUM(ol.qtyentered)   AS qtyreceived
				FROM
					c_orderline ol
						JOIN params
							ON params.ad_client_id = ol.ad_client_id
						AND date(dateordered) BETWEEN begin_date AND end_date
						JOIN c_order o
							ON ol.c_order_id = o.c_order_id
				WHERE
					o.issotrx = 'N'
					AND o.docstatus IN ('CL', 'CO')
					AND ol.m_product_id IS NOT NULL
				GROUP BY ol.m_product_id, ol.m_attributesetinstance_id
			) AS stockreceived
					ON currentqty.m_product_id = stockreceived.m_product_id
				AND currentqty.m_attributesetinstance_id = stockreceived.m_attributesetinstance_id
				LEFT JOIN (
				SELECT
					c.m_product_id,
					c.m_attributesetinstance_id,
					c.currentcostprice
				FROM
					m_cost c
						JOIN params p
							ON c.ad_client_id = p.ad_client_id
						JOIN c_acctschema actsch
							ON c.c_acctschema_id = actsch.c_acctschema_id AND c.m_costtype_id = actsch.m_costtype_id
						JOIN m_costelement ce
							ON c.m_costelement_id = ce.m_costelement_id AND ce.costingmethod = actsch.costingmethod
				WHERE
					c.currentcostprice > 0
			) costs
					ON currentqty.m_product_id = costs.m_product_id
				AND currentqty.m_product_id = costs.m_attributesetinstance_id
				LEFT JOIN (
				SELECT
					m_product_id,
					m_attributesetinstance_id,
					SUM(movementqty) AS qtybalanced
				FROM
					m_transaction
						JOIN params
							ON params.ad_client_id = m_transaction.ad_client_id
						AND date(movementdate) BETWEEN begin_date AND end_date
				WHERE
					movementtype IN ('I+', 'I-')
				GROUP BY m_product_id, m_attributesetinstance_id
			) AS stocktakechange
					ON currentqty.m_product_id = stocktakechange.m_product_id
				AND currentqty.m_attributesetinstance_id = stocktakechange.m_attributesetinstance_id
				LEFT JOIN (
				SELECT
					inoutline.m_product_id,
					SUM(inoutline.movementqty) AS qtysold
				FROM
					m_inoutline inoutline
						INNER JOIN m_inout inout
							ON inoutline.m_inout_id = inout.m_inout_id
						JOIN params
							ON params.ad_client_id = inoutline.ad_client_id
						AND date(inout.movementdate) BETWEEN begin_date AND end_date
				WHERE
					inout.movementtype = 'C-'
				GROUP BY inoutline.m_product_id
			) AS stocksold
					ON currentqty.m_product_id = stocksold.m_product_id
		GROUP BY item_name, soldstock
	) p
WHERE
	endingstock > 0
	OR receivedstock > 0
	OR soldstock > 0
	OR balancestock > 0
	OR changedstock > 0
ORDER BY
	item_name;]]>
		</queryString>
		<field name="item_name" class="java.lang.String">
			<property name="com.jaspersoft.studio.field.label" value="item_name"/>
			<property name="com.jaspersoft.studio.field.tree.path" value="bh_stocktake_v"/>
		</field>
		<field name="endingstock" class="java.math.BigDecimal">
			<property name="com.jaspersoft.studio.field.label" value="endingstock"/>
		</field>
		<field name="receivedstock" class="java.math.BigDecimal">
			<property name="com.jaspersoft.studio.field.label" value="receivedstock"/>
		</field>
		<field name="soldstock" class="java.math.BigDecimal">
			<property name="com.jaspersoft.studio.field.label" value="soldstock"/>
		</field>
		<field name="balancestock" class="java.math.BigDecimal">
			<property name="com.jaspersoft.studio.field.label" value="balancestock"/>
		</field>
		<field name="changedstock" class="java.math.BigDecimal">
			<property name="com.jaspersoft.studio.field.label" value="changedstock"/>
		</field>
		<variable name="stockchanges" class="java.math.BigDecimal">
			<variableExpression><![CDATA[$F{changedstock}.subtract($F{soldstock})]]></variableExpression>
			<initialValueExpression><![CDATA[0]]></initialValueExpression>
		</variable>
		<variable name="endstock" class="java.math.BigDecimal">
			<variableExpression><![CDATA[$F{endingstock}]]></variableExpression>
		</variable>
	</subDataset>
	<parameter name="Begin Date" class="java.util.Date"/>
	<parameter name="End Date" class="java.util.Date"/>
	<parameter name="AD_CLIENT_ID" class="java.lang.Integer" isForPrompting="false"/>
	<queryString>
		<![CDATA[select currentqty.product as item_name
from (
         select m_product_id,
                product
         from bh_stocktake_v
         where ad_client_id = $P{AD_CLIENT_ID}
         group by m_product_id, product
     ) as currentqty
inner join
    (
select m_product_id,
       sum(movementqty) as qtychanged
from m_transaction
where ad_client_id =  $P{AD_CLIENT_ID}
    and (date(movementdate) BETWEEN date($P{Begin Date}) AND date($P{End Date}))
group by m_product_id
        ) as changesinqty on currentqty.m_product_id = changesinqty.m_product_id
group by item_name
limit 1;]]>
	</queryString>
