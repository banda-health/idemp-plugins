	<parameter name="orderid" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[select o.ad_org_id,
       logo.binarydata	as image,
       o.name                                                       as Client_name,
       coalesce(cl.address1, cl.address2, cl.address3, cl.address4) as address,
       coalesce(i.phone, i.phone2)                                  as phone,
       city
from ad_org o
         inner join ad_orginfo i on o.ad_org_id = i.ad_org_id
         left join c_location cl on i.c_location_id = cl.c_location_id
         left join c_order c ON c.ad_client_id = o.ad_client_id
         left join ad_image logo on i.logo_id = logo.ad_image_id
where c.c_order_id = $P{orderid};]]>
