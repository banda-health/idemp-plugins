		<parameter name="Begin Date" class="java.util.Date"/>
		<parameter name="End Date" class="java.util.Date"/>
		<parameter name="AD_CLIENT_ID" class="java.lang.Integer" isForPrompting="false"/>
		<parameter name="Payment Mode" class="java.lang.String"/>
		<parameter name="Patient Type" class="java.lang.String"/>
		<queryString language="SQL">
			<![CDATA[WITH params AS (
    SELECT
        $P{AD_CLIENT_ID} AS ad_client_id,
        date($P{Begin Date}) AS begin_date,
        date($P{End Date}) AS end_date,
        coalesce($P{Patient Type}, '') AS patient_type,
        coalesce($P{Payment Mode}, '') AS payment_mode
)
select
    u.name as cashier,
    sum(coalesce(billdetails.lineitemtotals, 0)) as bill_total,
    sum(coalesce(patient_payments.total_payments, 0) +
        coalesce(non_patient_payments.total, 0)) as total_payment
from (
    select co.c_order_id,
           sum(ol.linenetamt) as lineitemtotals,
           co.createdby as cashier_id
    from c_order co
        inner join c_bpartner cb on co.c_bpartner_id = cb.c_bpartner_id
        join c_orderline ol on co.c_order_id = ol.c_order_id
        cross join params p
    where ((co.ad_client_id = p.ad_client_id) AND (co.docstatus <> 'VO')
          AND (date(co.bh_visitdate) BETWEEN p.begin_date AND p.end_date)
          AND (p.patient_type = '' OR co.bh_patienttype = p.patient_type)
        )
        AND ol.c_charge_id IS NULL
    group by co.c_order_id, co.createdby
    order by 1
) as billdetails
    inner join (
        select bh_c_order_id
        from c_payment p
        inner join c_order co on co.c_order_id = p.bh_c_order_id
        cross join params par
        where ((p.ad_client_id = par.ad_client_id)
            and ((p.docstatus = 'CO')
            or (p.docstatus = 'DR' and p.bh_processing = 'Y')
            or (co.docstatus = 'CO' and p.bh_processing = 'N'))
            and (par.payment_mode = '' OR par.payment_mode IN ('I', 'W', 'T') OR (tendertype = par.payment_mode and par.payment_mode IN ('X', 'K', 'C', 'D', 'M')))
            )
        group by bh_c_order_id
        union
        select o.c_order_id
        from c_order o
            join c_orderline ol on ol.c_order_id = o.c_order_id
            join c_charge c on ol.c_charge_id = c.c_charge_id
            cross join params p
        where ol.c_charge_id IS NOT NULL
            and (p.payment_mode = '' OR p.payment_mode IN ('X', 'K', 'C', 'D', 'M') OR (c.bh_subtype = CASE WHEN p.payment_mode = 'T' THEN 'D' ELSE p.payment_mode END and p.payment_mode IN ('I', 'W', 'T')))
        order by 1
    ) as payment_filter on billdetails.c_order_id = payment_filter.bh_c_order_id
    left join (
        select p.bh_c_order_id as c_order_id,
               sum(p.payamt) as total_payments
        from c_payment p
            cross join params
        where p.ad_client_id = params.ad_client_id
        group by p.bh_c_order_id
    ) as patient_payments on billdetails.c_order_id = patient_payments.c_order_id
    left join (
        select ol.c_order_id,
               sum(ol.linenetamt) * -1 as total
        from c_orderline ol
            join c_order o on ol.c_order_id = o.c_order_id
            cross join params p
            join c_charge c on ol.c_charge_id = c.c_charge_id
        where date(o.bh_visitdate) between p.begin_date and p.end_date
        group by ol.c_order_id
    ) non_patient_payments on billdetails.c_order_id = non_patient_payments.c_order_id
    join ad_user u on u.ad_user_id = billdetails.cashier_id
group by u.name
order by u.name;]]>
		</queryString>
