		<parameter name="Begin Date" class="java.util.Date"/>
		<parameter name="End Date" class="java.util.Date"/>
		<parameter name="AD_CLIENT_ID" class="java.lang.Integer" isForPrompting="false"/>
		<parameter name="Payment Mode" class="java.lang.String"/>
		<parameter name="Patient Type" class="java.lang.String"/>
		<queryString language="SQL">
			<![CDATA[;WITH params AS (
     SELECT $P{AD_CLIENT_ID} AS ad_client_id,
            date($P{Begin Date}) AS start_date,
            date($P{End Date}) AS end_date,
            coalesce($P{Patient Type}, '''') AS patient_type,
            coalesce($P{Payment Mode}, '''') AS payment_mode
)
select u.name as cashier,
       sum(bill_total) as bill_total,
       sum(total_payment) as total_payment
from (
    select o.totallines as bill_total,
           sum(p.payamt) as total_payment,
           p.createdby as cashier_id
    from c_order o
        left join c_payment p on o.c_order_id = p.bh_c_order_id
        cross join params
    where o.ad_client_id = params.ad_client_id
      AND o.docstatus <> 'VO'
      AND date(o.bh_visitdate) BETWEEN params.start_date AND params.end_date
      AND (
          params.patient_type = '''' OR o.bh_patienttype = params.patient_type
          )
    GROUP BY o.c_order_id, o.c_bpartner_id, o.totallines, p.createdby
    UNION
    SELECT 0 as bill_total,
           payamt as total_payment,
           p.createdby as cashier_id
    FROM c_payment p
    JOIN params ON params.ad_client_id = p.ad_client_id
                       AND date(p.datetrx) BETWEEN params.start_date AND params.end_date
                       AND (
                           params.payment_mode = '''' OR p.tendertype = params.payment_mode
                           )
    WHERE bh_c_order_id IS NULL
) as transactions
    join ad_user u on u.ad_user_id = transactions.cashier_id
group by u.name
order by u.name]]>
		</queryString>
