	<queryString>
		<![CDATA[select
  invoice.c_order_id as bill_id,
  case when payment.tendertype = 'M' then 'MPESA'
       when payment.tendertype = 'X' then 'CASH'
       when payment.tendertype = 'L' then 'LOCAL FUND'
       end as name,
  sum(payment.payamt) as amount_tendered
from
  c_invoice invoice
  inner join c_payment payment on invoice.c_order_id = payment.bh_c_order_id
where
  payment.bh_c_order_id = $P{billId}
  and payment.c_invoice_id is not null
  group by invoice.c_order_id, payment.tendertype]]>
	</queryString>
