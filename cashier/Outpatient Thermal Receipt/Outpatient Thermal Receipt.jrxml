	<queryString>
		<![CDATA[select sorder.dateordered as bill_date, sorder.c_order_id as receipt_number, cashier.name as cashier_family_name,
       patient.name as patient_family_name, cashier.name as cashier
from c_order sorder
inner join c_invoice invoice on sorder.c_order_id = invoice.c_order_id
inner join c_payment payment on invoice.c_invoice_id = payment.c_invoice_id
inner join ad_user patient on invoice.ad_user_id = patient.ad_user_id
inner join ad_user cashier on sorder.salesrep_id = cashier.ad_user_id
where sorder.c_order_id =   $P{billId} ]]>
