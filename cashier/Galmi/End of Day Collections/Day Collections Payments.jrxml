	<queryString>
		<![CDATA[select
  sum(payment.payamt) as amount_tendered,
  cashier.name as name
from
  c_payment payment
  inner join ad_user cashier on payment.createdby = cashier.ad_user_id
where
  payment.ad_client_id = $P{AD_CLIENT_ID} and payment.createdby = cashier.ad_user_id
  and date(payment.created) = current_date
group by cashier.name;]]>
	</queryString>
