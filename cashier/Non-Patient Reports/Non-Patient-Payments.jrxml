	<parameter name="Begin Date" class="java.sql.Timestamp"/>
	<parameter name="End Date" class="java.sql.Timestamp"/>
	<parameter name="Mode" class="java.lang.String"/>
	<parameter name="Type" class="java.lang.String"/>
	<parameter name="AD_CLIENT_ID" class="java.lang.Integer" isForPrompting="false"/>
	<queryString>
		<![CDATA[WITH params AS (
	SELECT
	 $P{AD_CLIENT_ID} AS ad_client_id,
    date($P{Begin Date}) AS begin_date,
    date($P{End Date}) AS end_date,
  	COALESCE($P{Mode},'') AS Mode,
    COALESCE($P{Type},'') AS Type
),
	Visit_info AS (
		SELECT *
		FROM
			bh_get_visit_details($P{AD_CLIENT_ID}, DATE($P{Begin Date}), DATE($P{End Date}))
	),
	DirectPayments AS (
		SELECT
			c_order_id,
			SUM(payamt) AS TotalDirectPayments
		FROM
			bh_get_visit_payments($P{AD_CLIENT_ID}, DATE($P{Begin Date}), DATE($P{End Date}))
		GROUP BY c_order_id
	),
	non_patient_payments AS (
		SELECT
			c_order_id,
			ChargeType_name,
			bh_subtype,
			SUM(linenetamt) FILTER ( WHERE bh_subtype = 'I' ) * -1             AS insurance,
			SUM(linenetamt) FILTER ( WHERE bh_subtype = 'W' ) * -1             AS waiver,
			SUM(linenetamt) FILTER ( WHERE bh_subtype = 'D' ) * -1             AS donation,
			SUM(linenetamt) FILTER ( WHERE bh_subtype IN ('D', 'W', 'I')) * -1 AS TotalNonPayments,
			member_id,
			MemberName,
			ClaimNo,
			Relationship
		FROM
			bh_get_visit_non_patient_payments($P{AD_CLIENT_ID}, DATE($P{Begin Date}), DATE($P{End Date}))
		GROUP BY c_order_id, ChargeType_name, bh_subtype, member_id, MemberName, ClaimNo, Relationship
	),
	BillTotal AS (
		SELECT
			co.c_order_id,
			SUM(ol.linenetamt) AS lineitemtotals
		FROM
			c_order co
				JOIN c_orderline ol
					ON co.c_order_id = ol.c_order_id
				CROSS JOIN params p
		WHERE
			((co.ad_client_id = p.ad_client_id) AND (co.docstatus <> 'VO' AND co.docstatus <> 'DR')
				AND (co.bh_visitdate BETWEEN p.begin_date AND p.end_date)
				AND issotrx = 'Y'
				)
			AND ol.c_charge_id IS NULL
		GROUP BY co.c_order_id
		ORDER BY 1
	),
	referral AS (
		SELECT DISTINCT
			r.name  AS PatientType,
			r.value AS Referral,
			Referraltype.c_order_id
		FROM
			ad_ref_list r
				JOIN ad_reference a
					ON r.ad_reference_id = a.ad_reference_id
				JOIN c_order Referraltype
					ON value = Referraltype.bh_patienttype
				CROSS JOIN params p
		WHERE
			ad_reference_uu = '47d32afd-3b94-4caa-8490-f0f1a97494f7'
			AND Referraltype.ad_client_id = p.ad_client_id
	)
SELECT
	Visit_info.c_order_id,
	bh_visitdate                          AS billdate,
	patient_name,
	bh_patientid                          AS PatientNo,
	referral.PatientType                  AS PatientType,
	member_id,
	membername                            AS MemberName,
	relationship                          AS Relationship,
	claimno                               AS ClaimNo,
	COALESCE(BillTotal.lineitemtotals, 0) AS billtotal,
	COALESCE(insurance, 0)                AS Insurance,
	COALESCE(waiver, 0)                   AS waiver,
	COALESCE(donation, 0)                 AS donation,
	COALESCE(TotalDirectPayments, 0)      AS other,
	COALESCE(TotalNonPayments, 0)         AS NonPaytotal
FROM
	Visit_info
		LEFT JOIN non_patient_payments np
			ON Visit_info.c_order_id = np.c_order_id
		LEFT JOIN referral
			ON Visit_info.c_order_id = referral.c_order_id
		LEFT JOIN DirectPayments
			ON Visit_info.c_order_id = DirectPayments.c_order_id
		LEFT JOIN BillTotal
			ON Visit_info.c_order_id = BillTotal.c_order_id
		CROSS JOIN params p
WHERE
	(p.Mode = '' OR bh_subtype = p.Mode)
	AND (p.Type = '' OR ChargeType_name = p.Type)
ORDER BY
	billdate ASC, patient_name DESC;


]]>
