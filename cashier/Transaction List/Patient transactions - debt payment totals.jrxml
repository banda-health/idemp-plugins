		<parameter name="AD_CLIENT_ID" class="java.lang.Integer" isForPrompting="false"/>
		<queryString>
			<![CDATA[select cashier.name as cashiername,
       tendertype.name as paymentmode,
       sum(cp.payamt) as paymentamount
from c_payment cp
    inner join c_bpartner cb on cp.c_bpartner_id = cb.c_bpartner_id
    inner join ad_user cashier on cp.createdby = cashier.ad_user_id
    inner join AD_Ref_List tendertype on cp.tendertype = tendertype.value
    and AD_Reference_ID=214
where (cp.ad_client_id = $P{AD_CLIENT_ID})
      and (cp.c_invoice_id is null and cp.isallocated = 'N' and cp.bh_c_order_id = 0)
      and (date(cp.datetrx) BETWEEN date($P{Begin Date}) AND date($P{End Date}))
group by cashier.name, tendertype.name;]]>
		</queryString>
