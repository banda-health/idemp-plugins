			<property name="com.jaspersoft.studio.field.tree.path" value="c_bpartner"/>
		</field>
		<field name="billtotal" class="java.math.BigDecimal">
			<property name="com.jaspersoft.studio.field.label" value="billtotal"/>
			<property name="com.jaspersoft.studio.field.tree.path" value="c_invoice"/>
		</field>
	</subDataset>
	<parameter name="Begin Date" class="java.util.Date"/>
	<parameter name="End Date" class="java.util.Date"/>
	<parameter name="AD_CLIENT_ID" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[select date(cp.datetrx) as bill_date,
       cb.name as patientname,
       cb.totalopenbalance as totaldebt,
       cp.payamt as amountpaid
from c_payment cp
    inner join c_bpartner cb on cp.c_bpartner_id = cb.c_bpartner_id
where ((cp.ad_client_id = $P{AD_CLIENT_ID})
      AND (cp.c_invoice_id is null and cp.isallocated = 'N' and cp.bh_c_order_id = 0)
      AND (date(cp.datetrx) BETWEEN date($P{Begin Date}) AND date($P{End Date}))
    )
order by cb.name asc;]]>
	</queryString>
