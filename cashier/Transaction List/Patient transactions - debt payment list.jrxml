	<parameter name="Begin Date" class="java.util.Date"/>
	<parameter name="End Date" class="java.util.Date"/>
	<parameter name="AD_CLIENT_ID" class="java.lang.Integer" isForPrompting="false"/>
	<parameter name="Patient Type" class="java.lang.String"/>
	<parameter name="Payment Mode" class="java.lang.String"/>
	<queryString>
		<![CDATA[select
    billdetails.c_order_id,
    billdetails.bill_date as billdate,
    billdetails.patientname as patientname,
    billdetails.lineitemtotals as billtotal
from
(
select co.c_order_id,
       cb.c_bpartner_id,
       date(co.dateordered) as bill_date,
       cb.name as patientname,
       co.totallines as lineitemtotals,
       co.grandtotal as totals
from c_order co
    inner join c_bpartner cb on co.c_bpartner_id = cb.c_bpartner_id
where ((co.ad_client_id = $P{AD_CLIENT_ID})
      AND (date(co.dateordered) BETWEEN date($P{Begin Date}) AND date($P{End Date}))
      AND ($P{Patient Type} IS NULL OR $P{Patient Type} = '''' OR co.bh_patienttype = $P{Patient Type})
    )
) as billdetails
inner join
(
select bh_c_order_id
from c_payment p
where ((p.ad_client_id = $P{AD_CLIENT_ID}) and ((p.docstatus = 'CO') OR (p.docstatus = 'DR' and p.bh_processing = 'Y'))
	AND ($P{Payment Mode} IS NULL OR $P{Payment Mode} = '''' OR tendertype = $P{Payment Mode})
    )
group by bh_c_order_id
) as payments on billdetails.c_order_id = payments.bh_c_order_id;]]>
	</queryString>
