	<queryString language="SQL">
		<![CDATA[SELECT
  org.name,
  SUM(CASE WHEN corder.issotrx = 'Y' THEN corder.grandtotal ELSE 0 END) AS revenue, 
  SUM(CASE WHEN corder.issotrx = 'N' THEN corder.grandtotal ELSE 0 END) AS cost
FROM c_order corder 
INNER JOIN ad_org org ON corder.ad_org_id = org.ad_org_id
WHERE  corder.ad_client_id =  $P{AD_CLIENT_ID} 
AND (date(corder.dateordered) BETWEEN date($P{Begin Date}) AND date($P{End Date})) 
AND corder.docstatus = 'CO' 
GROUP BY org.name]]>
	</queryString>
