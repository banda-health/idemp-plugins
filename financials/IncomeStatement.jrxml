	<parameter name="AD_CLIENT_ID" class="java.lang.Integer" isForPrompting="false"/>
	<parameter name="Begin Date" class="java.util.Date"/>
	<parameter name="End Date" class="java.util.Date"/>
	<parameter name="SUBREPORT_DIR" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA[""]]></defaultValueExpression>
	</parameter>
	<queryString language="SQL">
		<![CDATA[select
    billdetails.name as name,
    coalesce(billdetails.saleslineitemtotals,0) as billtotal,
    coalesce(payments.revenuesales,0) as salesrevenue,
    coalesce(debtPayments.debtpaymentamount,0) as debtrevenue,
    coalesce(expenses.expenselineitemtotals,0) as expensetotal,
    coalesce(purchases.purchaselineitemtotals,0) as purchasetotal,
    coalesce(salesproducts.productsales,0) as productsales,
    coalesce(incomeservice.serviceincome,0) as serviceincome
from
(
select co.ad_client_id,
	client.name,
	sum(co.totallines) as saleslineitemtotals,
	sum(co.grandtotal) as salestotals
from c_order co
    INNER JOIN ad_client client ON co.ad_client_id = client.ad_client_id
where
    co.ad_client_id = $P{AD_CLIENT_ID} AND co.docstatus = 'CO'
    AND bh_isexpense = 'N'
    AND (date(co.dateordered) BETWEEN date($P{Begin Date}) AND date($P{End Date}))
group by co.ad_client_id, client.name
) as billdetails
inner join
(
select
  p.ad_client_id,
  sum(p.payamt) as revenuesales
from
  c_payment p
    inner join c_order co on p.bh_c_order_id = co.c_order_id
    and co.docstatus <> 'VO'
where
   ((p.ad_client_id = $P{AD_CLIENT_ID}) and ((p.docstatus = 'CO') OR (p.docstatus = 'DR' and p.bh_processing = 'Y'))
    AND (date(co.dateordered) BETWEEN  date($P{Begin Date}) AND date($P{End Date})) )
group by p.ad_client_id
) as payments on billdetails.ad_client_id = payments.ad_client_id
left join
(
select cp.ad_client_id,
       sum(cp.payamt) as debtpaymentamount
from c_payment cp
where (cp.ad_client_id = $P{AD_CLIENT_ID})
      AND (cp.c_invoice_id is null and cp.isallocated = 'N' and cp.bh_c_order_id = 0)
      AND (date(cp.datetrx) BETWEEN date($P{Begin Date}) AND date($P{End Date}))
group by cp.ad_client_id
) as debtPayments on billdetails.ad_client_id = debtPayments.ad_client_id
left join
(
select ci.ad_client_id,
       sum(ci.totallines) as expenselineitemtotals,
       sum(ci.grandtotal) as expensetotals
from c_invoice ci
where ci.ad_client_id = $P{AD_CLIENT_ID} AND ci.docstatus = 'CO'
      AND bh_isexpense = 'Y'
      AND (date(ci.dateordered) BETWEEN date($P{Begin Date}) AND date($P{End Date}))
group by ci.ad_client_id
) as expenses on billdetails.ad_client_id = expenses.ad_client_id
left join
(
select co.ad_client_id,
       sum(co.totallines) as purchaselineitemtotals,
       sum(co.grandtotal) as purchasetotals
from c_order co
where co.ad_client_id = $P{AD_CLIENT_ID} AND co.docstatus = 'CO'
      AND bh_isexpense is null
      AND (date(co.dateordered) BETWEEN date($P{Begin Date}) AND date($P{End Date}))
group by co.ad_client_id
) as purchases on billdetails.ad_client_id = purchases.ad_client_id
left join
(
select coline.ad_client_id,
    sum(coline.linenetamt) as productsales
from c_orderline coline
    inner join m_product product on coline.m_product_id = product.m_product_id
    and product.producttype = 'I'
    inner join c_order on coline.c_order_id = c_order.c_order_id
    and bh_isexpense = 'N'
where coline.ad_client_id = $P{AD_CLIENT_ID}
    AND coline.processed = 'Y'
    AND (date(coline.dateordered) BETWEEN date($P{Begin Date})
    AND date($P{End Date}))
group by coline.ad_client_id
) as salesproducts on billdetails.ad_client_id = salesproducts.ad_client_id
left join
(
select coline.ad_client_id,
       sum(coline.linenetamt) as serviceincome
from c_orderline coline
    inner join m_product product on coline.m_product_id = product.m_product_id
    and product.producttype = 'S'
    inner join c_order on coline.c_order_id = c_order.c_order_id
    and bh_isexpense = 'N'
where coline.ad_client_id = $P{AD_CLIENT_ID} AND coline.processed = 'Y'
      AND (date(coline.dateordered) BETWEEN date($P{Begin Date}) AND date($P{End Date}))
group by coline.ad_client_id
) as incomeservice on billdetails.ad_client_id = incomeservice.ad_client_id;]]>
