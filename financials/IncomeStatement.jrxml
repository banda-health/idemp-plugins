	</parameter>
	<queryString language="SQL">
		<![CDATA[WITH params AS (
	SELECT
		$P{AD_CLIENT_ID}     AS ad_client_id,
		DATE($P{Begin Date}) AS begin_date,
		DATE($P{End Date})   AS end_date
)
SELECT
	c.name                                                                       AS name,
	COALESCE(billdetails.saleslineitemtotals, 0)                                 AS billtotal,
	COALESCE(payments.revenuesales, 0)                                           AS salesrevenue,
	COALESCE(debtPayments.debtpaymentamount, 0)                                  AS debtrevenue,
	COALESCE(expenses.totalexpenses, 0),
		COALESCE(expenses.totalexpenses, 0) + COALESCE(received_products.total, 0) AS expensetotal,
	COALESCE(charges.pharmacysales, 0)                                           AS pharmacysales,
	COALESCE(charges.servicessales, 0)                                           AS servicessales,
	COALESCE(charges.labsales, 0)                                                AS labsales,
	COALESCE(charges.radiologysales, 0)                                          AS radiologysales,
	COALESCE(charges.othersales, 0)                                              AS othersales,
	COALESCE(payments.cash, 0)                                                   AS cash,
	COALESCE(payments.mobile, 0)                                                 AS mobile,
	COALESCE(payments.credit_debit, 0)                                           AS credit_debit,
	COALESCE(payments.bank_transfer, 0)                                          AS bank_transfer,
	COALESCE(payments.check, 0)                                                  AS "check",
	COALESCE(payments.other, 0)                                                  AS otherpayments,
	COALESCE(non_patient_payments.waivers, 0) * -1                               AS waivers,
	COALESCE(non_patient_payments.donations, 0) * -1                             AS donations,
	COALESCE(non_patient_payments.insurance, 0) * -1                             AS insurance
FROM
	ad_client c
		JOIN params p
			ON c.ad_client_id = p.ad_client_id
		CROSS JOIN (
		SELECT
			SUM(saleslineitemtotals) AS saleslineitemtotals,
			SUM(salestotals)         AS salestotals
		FROM
			bh_get_visit_details($P{AD_CLIENT_ID}, $P{Begin Date}::timestamp, $P{End Date}::timestamp)
	) AS billdetails
		CROSS JOIN (
		SELECT
			COALESCE(SUM(payamt), 0)                                                              AS revenuesales,
			COALESCE(SUM(payamt) FILTER ( WHERE tendertype = 'X' ), 0)                            AS cash,
			COALESCE(SUM(payamt) FILTER ( WHERE tendertype = 'M' ), 0)                            AS mobile,
			COALESCE(SUM(payamt) FILTER ( WHERE tendertype = 'C' ), 0)                            AS credit_debit,
			COALESCE(SUM(payamt) FILTER ( WHERE tendertype = 'D' ), 0)                            AS bank_transfer,
			COALESCE(SUM(payamt) FILTER ( WHERE tendertype = 'K' ), 0)                            AS "check",
			COALESCE(SUM(payamt) FILTER ( WHERE tendertype NOT IN ('X', 'M', 'C', 'D', 'K') ), 0) AS other
		FROM
			bh_get_visit_payments($P{AD_CLIENT_ID}, $P{Begin Date}::timestamp, $P{End Date}::timestamp)
	) AS payments
		CROSS JOIN (
		SELECT
			COALESCE(SUM(linenetamt) FILTER ( WHERE bh_subtype = 'W' ), 0) AS waivers,
			COALESCE(SUM(linenetamt) FILTER ( WHERE bh_subtype = 'D' ), 0) AS donations,
			COALESCE(SUM(linenetamt) FILTER ( WHERE bh_subtype = 'I' ), 0) AS insurance
		FROM
			bh_get_visit_non_patient_payments($P{AD_CLIENT_ID}, $P{Begin Date}::timestamp, $P{End Date}::timestamp)
	) AS non_patient_payments
		CROSS JOIN (
		SELECT
			COALESCE(SUM(p.payamt), 0) AS debtpaymentamount
		FROM
			c_payment p
				JOIN params
					ON p.ad_client_id = params.ad_client_id
				LEFT JOIN c_allocationline al
					ON p.c_payment_id = al.c_payment_id
				LEFT JOIN c_invoice i
					ON al.c_invoice_id = i.c_invoice_id
		WHERE
			((p.docstatus = 'CO') OR (p.docstatus = 'DR' AND p.bh_processing = 'Y'))
			AND (p.bh_c_order_id = 0 OR bh_c_order_id IS NULL)
			AND (DATE(p.datetrx) BETWEEN params.begin_date AND params.end_date)
			AND (i.docstatus IS NULL OR i.docstatus NOT IN ('RE', 'RA', 'VO'))
	) AS debtPayments
		CROSS JOIN (
		SELECT
			COALESCE(SUM(i.grandtotal), 0) AS totalexpenses
		FROM
			c_invoice i
				JOIN params
					ON i.ad_client_id = params.ad_client_id
		WHERE
			i.docstatus = 'CO'
			AND i.bh_isexpense = 'Y'
			AND (date(i.dateinvoiced) BETWEEN params.begin_date AND params.end_date)
	) AS expenses
		CROSS JOIN (
		SELECT
			COALESCE(SUM(ol.linenetamt)
			         FILTER ( WHERE pc.name IN ('Laboratory', 'Pharmacy', 'Radiology', 'Other', 'Standard') ),
			         0) AS total
		FROM
			c_order o
				JOIN c_orderline ol
					ON o.c_order_id = ol.c_order_id
				JOIN m_product p
					ON ol.m_product_id = p.m_product_id
				JOIN m_product_category pc
					ON p.m_product_category_id = pc.m_product_category_id
				JOIN params
					ON o.ad_client_id = params.ad_client_id
		WHERE
			o.issotrx = 'N'
			AND o.bh_isexpense IS NULL
			AND o.docstatus = 'CO'
			AND (date(o.dateordered) BETWEEN params.begin_date AND params.end_date)
	) received_products
		CROSS JOIN (
		SELECT
			COALESCE(SUM(ol.linenetamt) FILTER ( WHERE pc.name = 'Pharmacy' ), 0)   AS pharmacysales,
			COALESCE(SUM(ol.linenetamt) FILTER ( WHERE pc.name = 'Services' ), 0)   AS servicessales,
			COALESCE(SUM(ol.linenetamt) FILTER ( WHERE pc.name = 'Laboratory' ), 0) AS labsales,
			COALESCE(SUM(ol.linenetamt) FILTER ( WHERE pc.name = 'Radiology' ), 0)  AS radiologysales,
			COALESCE(SUM(ol.linenetamt) FILTER ( WHERE pc.name = 'Other' ), 0)      AS othersales
		FROM
			c_order o
				INNER JOIN c_orderline ol
					ON o.c_order_id = ol.c_order_id
				INNER JOIN m_product p
					ON ol.m_product_id = p.m_product_id
				RIGHT JOIN m_product_category pc
					ON p.m_product_category_id = pc.m_product_category_id
				JOIN params
					ON o.ad_client_id = params.ad_client_id
		WHERE
			o.docstatus = 'CO'
			AND o.bh_isexpense = 'N'
			AND (DATE(o.bh_visitdate) BETWEEN params.begin_date AND params.end_date)
	) AS charges;
]]>
	</queryString>
