	<parameter name="AD_CLIENT_ID" class="java.lang.Integer" isForPrompting="false"/>
	<parameter name="Begin Date" class="java.util.Date"/>
	<parameter name="End Date" class="java.util.Date"/>
	<parameter name="SUBREPORT_DIR" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA[""]]></defaultValueExpression>
	</parameter>
	<queryString language="SQL">
		<![CDATA[WITH params AS (
    SELECT
        $P{AD_CLIENT_ID} as ad_client_id,
        DATE($P{Begin Date}) as start_date,
        DATE($P{End Date}) as end_date
)
SELECT
    billdetails.name AS name,
    coalesce(billdetails.saleslineitemtotals,0) AS billtotal,
    coalesce(payments.revenuesales,0) AS salesrevenue,
    coalesce(debtPayments.debtpaymentamount,0) AS debtrevenue,
    coalesce(expenses.totalexpenses,0) + coalesce(received_products.total,0) AS expensetotal,
    coalesce(charges.pharmacysales,0) AS pharmacysales,
    coalesce(charges.servicessales,0) AS servicessales,
    coalesce(charges.labsales,0) AS labsales,
    coalesce(charges.radiologysales,0) AS radiologysales,
    coalesce(charges.othersales,0) AS othersales,
    coalesce(payments.cash,0) as cash,
    coalesce(payments.mobile, 0) as mobile,
    coalesce(payments.credit_debit,0) as credit_debit,
    coalesce(payments.bank_transfer, 0) as bank_transfer,
    coalesce(payments.check, 0) as "check",
    coalesce(payments.other, 0) as otherpayments,
    coalesce(non_patient_payments.waivers, 0) * -1 as waivers,
    coalesce(non_patient_payments.donations, 0) * -1 as donations,
    coalesce(non_patient_payments.insurance, 0) * -1 as insurance
FROM (
    SELECT
        co.ad_client_id,
        client.name,
        sum(ol.linenetamt) FILTER ( WHERE ol.c_charge_id IS NULL ) AS saleslineitemtotals,
        sum(co.grandtotal) AS salestotals
    FROM c_order co
        INNER JOIN ad_client client ON co.ad_client_id = client.ad_client_id
        JOIN c_orderline ol ON co.c_order_id = ol.c_order_id
    JOIN params ON co.ad_client_id = params.ad_client_id
    WHERE
        co.docstatus = 'CO'
        AND co.bh_isexpense = 'N'
        AND (DATE(co.bh_visitdate) BETWEEN params.start_date AND params.end_date)
    GROUP BY 1,2
) AS billdetails
INNER JOIN (
    SELECT
        p.ad_client_id,
        sum(p.payamt) AS revenuesales,
        sum(p.payamt) FILTER ( WHERE p.tendertype = 'X' ) as cash,
        sum(p.payamt) FILTER ( WHERE p.tendertype = 'M' ) as mobile,
        sum(p.payamt) FILTER ( WHERE p.tendertype = 'C' ) as credit_debit,
        sum(p.payamt) FILTER ( WHERE p.tendertype = 'D' ) as bank_transfer,
        sum(p.payamt) FILTER ( WHERE p.tendertype = 'K' ) as "check",
        sum(p.payamt) FILTER ( WHERE p.tendertype NOT IN ('X','M','C', 'D', 'K') ) as other
    FROM c_payment p
    INNER JOIN c_order co ON p.bh_c_order_id = co.c_order_id
    JOIN params ON p.ad_client_id = params.ad_client_id
    WHERE
        (((p.docstatus = 'CO') OR (p.docstatus = 'DR' AND p.bh_processing = 'Y'))
        AND (date(co.bh_visitdate) BETWEEN params.start_date AND params.end_date)
        AND co.docstatus IN ('CO','DR'))
    GROUP BY 1
) AS payments on billdetails.ad_client_id = payments.ad_client_id
JOIN (
    SELECT
        ol.ad_client_id,
        sum(ol.linenetamt) FILTER ( WHERE c.bh_subtype = 'W' ) as waivers,
        sum(ol.linenetamt) FILTER ( WHERE c.bh_subtype = 'D' ) as donations,
        sum(ol.linenetamt) FILTER ( WHERE c.bh_subtype = 'I' ) as insurance
    FROM c_orderline ol
    JOIN c_order o ON ol.c_order_id = o.c_order_id
    JOIN params ON ol.ad_client_id = params.ad_client_id
    JOIN c_charge c ON ol.c_charge_id = c.c_charge_id
    WHERE
        (date(o.bh_visitdate) BETWEEN params.start_date AND params.end_date)
        AND o.docstatus IN ('CO','DR')
    GROUP BY 1
) AS non_patient_payments on billdetails.ad_client_id = payments.ad_client_id
LEFT JOIN (
    SELECT
        cp.ad_client_id,
        sum(cp.payamt) AS debtpaymentamount
    FROM c_payment cp
    JOIN params ON cp.ad_client_id = params.ad_client_id
    WHERE
        ((cp.docstatus = 'CO') OR (cp.docstatus = 'DR' AND cp.bh_processing = 'Y'))
        AND (cp.c_invoice_id IS NULL AND cp.isallocated = 'N' AND cp.bh_c_order_id = 0)
        AND (DATE(cp.datetrx) BETWEEN params.start_date AND params.end_date)
    GROUP BY 1
) AS debtPayments ON billdetails.ad_client_id = debtPayments.ad_client_id
LEFT JOIN (
    SELECT
        ci.ad_client_id,
        SUM(ci.grandtotal) as totalexpenses
    FROM c_invoice ci
    JOIN params ON ci.ad_client_id = params.ad_client_id
    WHERE
        ci.docstatus = 'CO'
        AND ci.bh_isexpense = 'Y'
        AND (date(ci.dateinvoiced) BETWEEN params.start_date AND params.end_date)
    GROUP BY 1
) AS expenses ON billdetails.ad_client_id = expenses.ad_client_id
LEFT JOIN (
    SELECT
        o.ad_client_id,
        coalesce(sum(ol.linenetamt) FILTER ( WHERE pc.name IN ('Laboratory', 'Pharmacy', 'Radiology', 'Other', 'Standard') ), 0) AS total
    FROM c_order o
    JOIN c_orderline ol ON o.c_order_id = ol.c_order_id
    JOIN m_product p ON ol.m_product_id = p.m_product_id
    JOIN m_product_category pc ON p.m_product_category_id = pc.m_product_category_id
    JOIN params ON o.ad_client_id = params.ad_client_id
    WHERE o.issotrx = 'N'
        AND o.bh_isexpense IS NULL
        AND o.docstatus = 'CO'
        AND (date(o.bh_visitdate) BETWEEN params.start_date AND params.end_date)
    GROUP BY 1
) received_products ON billdetails.ad_client_id = received_products.ad_client_id
LEFT JOIN (
    SELECT
        co.ad_client_id,
        sum(coline.linenetamt) FILTER ( WHERE mpc.value = 'Pharmacy' ) AS pharmacysales,
        sum(coline.linenetamt) FILTER ( WHERE mpc.value = 'Services' ) AS servicessales,
        sum(coline.linenetamt) FILTER ( WHERE mpc.value = 'Laboratory' ) AS labsales,
        sum(coline.linenetamt) FILTER ( WHERE mpc.value = 'Radiology' ) AS radiologysales,
        sum(coline.linenetamt) FILTER ( WHERE mpc.value = 'Other' ) AS othersales
    FROM c_order co
        INNER JOIN c_orderline coline ON co.c_order_id = coline.c_order_id
        INNER JOIN m_product product ON coline.m_product_id = product.m_product_id
        RIGHT JOIN m_product_category mpc ON product.m_product_category_id = mpc.m_product_category_id
    JOIN params ON co.ad_client_id = params.ad_client_id
    WHERE
        co.docstatus = 'CO'
        AND co.bh_isexpense = 'N'
        AND (DATE(co.bh_visitdate) BETWEEN params.start_date AND params.end_date)
    GROUP BY 1
) AS charges ON billdetails.ad_client_id = charges.ad_client_id
;]]>
	</queryString>
