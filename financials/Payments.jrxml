	<queryString language="SQL">
		<![CDATA[SELECT bp.c_bpartner_id,
       bp.name as name,
       SUM(ci.grandtotal) AS total_supplied
FROM c_invoice ci
    INNER JOIN c_bpartner bp ON ci.c_bpartner_id = bp.c_bpartner_id
WHERE
      ci.docstatus = 'CO'
      AND ci.issotrx = 'N'
      AND ci.ad_client_id = $P{AD_CLIENT_ID}
      AND (date(ci.dateinvoiced) BETWEEN date($P{Begin Date}) AND date($P{End Date}))
GROUP BY bp.c_bpartner_id;]]>
