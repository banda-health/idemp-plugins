	<subDataset name="Referral" uuid="d7f308c7-8efb-48c0-8987-a537a9593541">
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="LocalDB Adapter"/>
		<parameter name="AD_CLIENT_ID" class="java.lang.Integer"/>
		<parameter name="Begin Date" class="java.sql.Date"/>
		<parameter name="End Date" class="java.sql.Date"/>
		<queryString language="SQL">
			<![CDATA[WITH date_limitter AS (
    SELECT generate_series(date($P{Begin Date}), date($P{End Date}), '1d')::date AS calendar
),
     referral_list AS (
         SELECT DISTINCT value as Referral
         FROM ad_ref_list
                  JOIN c_order Referraltype ON value = Referraltype.bh_referral
                  RIGHT JOIN ad_reference a ON ad_ref_list.ad_reference_id = a.ad_reference_id
         WHERE  Referraltype.ad_client_id = $P{AD_CLIENT_ID}
         AND value is not null
     ),
     referral_count AS (
         SELECT DISTINCT bh_moh705a_lessthan5,
                         value                       as referral,
                         c.calendar,
                         coalesce(c.noofreferral, 0) as noofreferral
         FROM ad_ref_list b
                  LEFT JOIN (
             SELECT bh_moh705a_lessthan5,
                    bh_referral,
                    cast(bh_visitdate as date)         as calendar,
                    COUNT(bh_primarycodeddiagnosis_id) AS noofreferral
             from c_order c
                      JOIN bh_coded_diagnosis cd ON bh_primarycodeddiagnosis_id::numeric = cd.bh_coded_diagnosis_id
                      JOIN date_limitter dl ON dl.calendar = cast(bh_visitdate as date)
--                       JOIN ad_org org ON c.ad_org_id = org.ad_org_id
             WHERE c.ad_client_id = $P{AD_CLIENT_ID}
             AND bh_primarycodeddiagnosis_id IS NOT NULL
             AND bh_moh705a_lessthan5 NOT IN ('NA', 'COVID-19 - suspected', 'COVID-19 - confirmed', 'Overweight (BMI>25)',
                                       'Other Diseases Of Respiratory System','Tuberculosis','Sexually Transmitted Infections', 'Fevers',
                                      'Chromosomal Abnormalities (E.G Downs Syndrome; Edwards Syndrome Etc.)', 'Upper Respiratory Infection',
                                      'Other Eye Conditions', 'Plague','Arthritis; Joint pains; etc','Diarrhea','Kalaazar','Congenital Anomalies',
                                      'Ear infections / conditions','Newly Diagnosed HIV','Tetanus (not neonatal)','Sexual Assault','Dracunculiasis (Guinea Worm)',
                                      'Neoplasms', 'Physical Disability','Cardiovascular Conditions','Musculoskeletal Conditions','Other Central Nervous System Conditions')
             GROUP BY bh_referral,bh_moh705a_lessthan5, cast(bh_visitdate as date)
         ) c ON b.value = c.bh_referral
         ORDER BY b.value
     )
SELECT dali.calendar,
       bh_moh705a_lessthan5,
       dl.Referral,
       COALESCE(dc.noofreferral, 0) as noofreferral
FROM referral_list dl
         CROSS JOIN date_limitter dali
         LEFT JOIN referral_count dc
                   ON dc.calendar = dali.calendar
                       AND dl.referral = dc.referral
ORDER BY dl.Referral ASC;]]>
		</queryString>
		<field name="calendar" class="java.sql.Date">
			<property name="com.jaspersoft.studio.field.label" value="calendar"/>
		</field>
		<field name="referral" class="java.lang.String">
			<property name="com.jaspersoft.studio.field.label" value="referral"/>
			<property name="com.jaspersoft.studio.field.tree.path" value="ad_ref_list"/>
		</field>
		<field name="noofreferral" class="java.lang.Integer">
			<property name="com.jaspersoft.studio.field.label" value="noofreferral"/>
		</field>
		<group name="referral">
			<groupExpression><![CDATA[$F{referral}]]></groupExpression>
		</group>
	</subDataset>
	<parameter name="Begin Date" class="java.sql.Date"/>
	<parameter name="End Date" class="java.sql.Date"/>
	<parameter name="AD_CLIENT_ID" class="java.lang.Integer" isForPrompting="false"/>
	<queryString>
		<![CDATA[select
    date(c.bh_visitdate) as calendar,
    client.name AS name
from c_order c
    inner join bh_coded_diagnosis b ON b.bh_coded_diagnosis_id = c.bh_primarycodeddiagnosis_id
    right join ad_org client on c.ad_client_id = client.ad_client_id
     and client.isactive = 'Y'
     where  c.ad_client_id  = $P{AD_CLIENT_ID}
 GROUP BY c.bh_visitdate, client.name, b.bh_moh705a_lessthan5
 ORDER BY calendar ASC]]>
	</queryString>
	<field name="calendar" class="java.sql.Date"/>
	<field name="name" class="java.lang.String"/>
