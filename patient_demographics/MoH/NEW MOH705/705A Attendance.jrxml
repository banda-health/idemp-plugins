	<subDataset name="705A Attendance" uuid="b400fc7b-e610-4255-a60d-0acfc0edbea7">
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="LocalDB Adapter"/>
		<parameter name="Begin Date" class="java.util.Date"/>
		<parameter name="AD_CLIENT_ID" class="java.lang.Integer"/>
		<queryString language="SQL">
			<![CDATA[WITH date_limitter AS (
    SELECT generate_series(date($P{Begin Date}), date($P{Begin Date}) + interval '1 month' - interval '1 day', interval '1 day') AS calendar
),
     new_list AS (
         SELECT DISTINCT value as  Attendance
         FROM ad_ref_list
                  JOIN c_order attendancetype ON value = attendancetype.bh_newvisit
                  RIGHT JOIN ad_reference a ON ad_ref_list.ad_reference_id = a.ad_reference_id
         WHERE  attendancetype.ad_client_id =  $P{AD_CLIENT_ID}
         AND value is not null
     ),
     attendance_count AS (
         SELECT DISTINCT bh_moh705a_lessthan5,
                         b.value                       as attendance,
                         c.calendar,
                         coalesce(c.noofattendance, 0) as noofattendance
         FROM ad_ref_list b
                  LEFT JOIN (
             SELECT bh_moh705a_lessthan5,
                    bh_newvisit,
                    cast(bh_visitdate as date)         as calendar,
                    COUNT(bh_primarycodeddiagnosis_id) AS noofattendance
             from c_order c
                      JOIN bh_coded_diagnosis cd ON bh_primarycodeddiagnosis_id = cd.bh_coded_diagnosis_id
                      JOIN date_limitter dl ON dl.calendar = cast(bh_visitdate as date)
                      JOIN c_bpartner bp ON c.c_bpartner_id = bp.c_bpartner_id
--                       JOIN ad_org org ON c.ad_org_id = org.ad_org_id
             WHERE c.ad_client_id =  $P{AD_CLIENT_ID}
                AND bp.bh_birthday IS NOT NULL AND date_part('year', age(bp.bh_birthday)) < 5
             GROUP BY bh_newvisit,bh_moh705a_lessthan5, cast(bh_visitdate as date)
         ) c ON b.value = bh_newvisit
         ORDER BY b.value
     )
SELECT dali.calendar,
      bh_moh705a_lessthan5,
       dl.Attendance,
       COALESCE(dc.noofattendance, 0) as noofreferral
FROM new_list dl
         CROSS JOIN date_limitter dali
         LEFT JOIN attendance_count dc
                   ON dc.calendar = dali.calendar
                       AND dl.Attendance = dc.attendance
ORDER BY dl.Attendance ASC;]]>
		</queryString>
		<field name="calendar" class="java.sql.Timestamp">
			<property name="com.jaspersoft.studio.field.label" value="calendar"/>
		</field>
		<field name="attendance" class="java.lang.String">
			<property name="com.jaspersoft.studio.field.label" value="attendance"/>
			<property name="com.jaspersoft.studio.field.tree.path" value="ad_ref_list"/>
		</field>
		<field name="noofreferral" class="java.lang.Long">
			<property name="com.jaspersoft.studio.field.label" value="noofreferral"/>
		</field>
		<group name="attendance">
			<groupExpression><![CDATA[$F{attendance}]]></groupExpression>
		</group>
	</subDataset>
	<parameter name="Begin Date" class="java.util.Date"/>
	<parameter name="AD_CLIENT_ID" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[select
    date(c.bh_visitdate) as calendar,
    client.name AS name
from c_order c
    left join bh_coded_diagnosis b ON b.bh_coded_diagnosis_id = c.bh_primarycodeddiagnosis_id
    right join ad_org client on c.ad_client_id = client.ad_client_id
     and client.isactive = 'Y'
     where  c.ad_client_id  = $P{AD_CLIENT_ID}
    and b.bh_moh705a_lessthan5 is not null
 GROUP BY c.bh_visitdate, client.name, b.bh_moh705a_lessthan5]]>
	</queryString>
	<field name="calendar" class="java.sql.Date"/>
	<field name="name" class="java.lang.String"/>
